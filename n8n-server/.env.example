# PostgreSQL main credentials
POSTGRES_USER=postgres
POSTGRES_PASSWORD=CHANGEME
POSTGRES_DB=n8n

# PostgreSQL non-root app user
POSTGRES_NON_ROOT_USER=n8n_user
POSTGRES_NON_ROOT_PASSWORD=CHANGEME

# n8n URL settings (important behind Cloudflare)
# Example: automation.mycompany.com
N8N_HOST=your-domain.com

# Example: https://automation.mycompany.com/
WEBHOOK_URL=https://your-domain.com/

# Generate a strong encryption key (32 chars)
# Example: openssl rand -hex 32
N8N_ENCRYPTION_KEY=REPLACE_ME
